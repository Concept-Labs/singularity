{
    "name": "concept-labs/singularity",
    "description": "(C)oncept-Labs Singularity. Dependency Injection pack",
    "keywords": ["php", "psr-11", "di", "container", "dependency injection", "singularity"],
    "license": "MIT",
    "authors": [
        {
            "name": "Viktor Halytskyi",
            "email": "concept.galitsk@gmail.com"
        }
    ],
    "require": {
        "php": ">=8.2",
        "psr/container": "^2",
        "concept-labs/config": "^2",
        "concept-labs/simple-cache": "^1",
        "concept-labs/composer": "^1"
    },
    "autoload": {
        "psr-4": {
            "Concept\\Singularity\\": "src/"
        }
    },
    "require-dev": {
        "pestphp/pest": "^2.0"
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests/"
        }
    },
    "config": {
        "allow-plugins": {
            "pestphp/pest-plugin": true
        }
    },
    "scripts": {
        "test": "pest"
    },
    "extra": {
        "concept": {
            
        }
    }

}